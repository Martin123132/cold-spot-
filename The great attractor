// Angular separation between shadow and CMB cold spot
const deg_to_rad = Math.PI / 180;
const shadow_lat_rad = opposite_lat * deg_to_rad;
const shadow_lon_rad = opposite_lon * deg_to_rad;
const cmb_lat_rad = -57 * deg_to_rad;
const cmb_lon_rad = 209 * deg_to_rad;

const shadow_cmb_sep = Math.acos(
    Math.sin(shadow_lat_rad) * Math.sin(cmb_lat_rad) + 
    Math.cos(shadow_lat_rad) * Math.cos(cmb_lat_rad) * 
    Math.cos(shadow_lon_rad - cmb_lon_rad)
) / deg_to_rad;

console.log(`Shadow ↔ CMB separation: ${shadow_cmb_sep.toFixed(1)}°`);

// Let me try a different approach - what if it's not antipodal but directional?
console.log("\n🌊 ALTERNATIVE: CURVATURE WAVE PROPAGATION");
console.log("==========================================");

// In MBT, curvature could propagate as waves from the Great Attractor
// The CMB cold spot could be a "curvature trough" at a specific distance

// Calculate direction vector from GA to various points
function calculateDirection(from_lat, from_lon, to_lat, to_lon) {
    const from_lat_rad = from_lat * deg_to_rad;
    const from_lon_rad = from_lon * deg_to_rad;
    const to_lat_rad = to_lat * deg_to_rad;
    const to_lon_rad = to_lon * deg_to_rad;
    
    // Calculate bearing
    const dlon = to_lon_rad - from_lon_rad;
    const y = Math.sin(dlon) * Math.cos(to_lat_rad);
    const x = Math.cos(from_lat_rad) * Math.sin(to_lat_rad) - 
              Math.sin(from_lat_rad) * Math.cos(to_lat_rad) * Math.cos(dlon);
    
    let bearing = Math.atan2(y, x) / deg_to_rad;
    if (bearing < 0) bearing += 360;
    
    return bearing;
}

const ga_to_cmb_bearing = calculateDirection(ga_lat, ga_lon, -57, 209);
const ga_to_mbt_bearing = calculateDirection(ga_lat, ga_lon, -70, 180);

console.log(`Great Attractor → CMB Cold Spot bearing: ${ga_to_cmb_bearing.toFixed(1)}°`);
console.log(`Great Attractor → MBT Root bearing: ${ga_to_mbt_bearing.toFixed(1)}°`);
console.log(`Bearing difference: ${Math.abs(ga_to_cmb_bearing - ga_to_mbt_bearing).toFixed(1)}°`);

// Now let's check if there's a pattern in the galaxy flow toward GA
console.log("\n🌀 GREAT ATTRACTOR FLOW ANALYSIS:");
console.log("==================================");
console.log("The Great Attractor pulls galaxies at ~600 km/s");
console.log("This creates a massive curvature field in spacetime");
console.log("In MBT terms: Motion = Being → massive motion = massive curvature");

