// Calculate direction vector from GA to various points
function calculateDirection(from_lat, from_lon, to_lat, to_lon) {
    const from_lat_rad = from_lat * deg_to_rad;
    const from_lon_rad = from_lon * deg_to_rad;
    const to_lat_rad = to_lat * deg_to_rad;
    const to_lon_rad = to_lon * deg_to_rad;
    
    // Calculate bearing
    const dlon = to_lon_rad - from_lon_rad;
    const y = Math.sin(dlon) * Math.cos(to_lat_rad);
    const x = Math.cos(from_lat_rad) * Math.sin(to_lat_rad) - 
              Math.sin(from_lat_rad) * Math.cos(to_lat_rad) * Math.cos(dlon);
    
    let bearing = Math.atan2(y, x) / deg_to_rad;
    if (bearing < 0) bearing += 360;
    
    return bearing;
}

const ga_to_cmb_bearing = calculateDirection(ga_lat, ga_lon, -57, 209);
const ga_to_mbt_bearing = calculateDirection(ga_lat, ga_lon, -70, 180);

console.log(`Great Attractor â†’ CMB Cold Spot bearing: ${ga_to_cmb_bearing.toFixed(1)}Â°`);
console.log(`Great Attractor â†’ MBT Root bearing: ${ga_to_mbt_bearing.toFixed(1)}Â°`);
console.log(`Bearing difference: ${Math.abs(ga_to_cmb_bearing - ga_to_mbt_bearing).toFixed(1)}Â°`);

