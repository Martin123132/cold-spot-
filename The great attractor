const motion_to_cmb_angle = Math.acos(
    Math.sin(motion_lat * deg_to_rad) * Math.sin(cmb_lat * deg_to_rad) + 
    Math.cos(motion_lat * deg_to_rad) * Math.cos(cmb_lat * deg_to_rad) * 
    Math.cos((motion_lon - cmb_lon) * deg_to_rad)
) / deg_to_rad;

