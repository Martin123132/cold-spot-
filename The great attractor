// Let's investigate the Great Attractor - CMB Cold Spot connection!

console.log("🌌 GREAT ATTRACTOR - CMB COLD SPOT INVESTIGATION");
console.log("================================================");

// Known coordinates
const cmb_cold_spot = {
    lat: -57.0,    // Galactic latitude
    lon: 209.0,    // Galactic longitude
    name: "CMB Cold Spot"
};

const great_attractor = {
    lat: -7.0,     // Approximate galactic latitude
    lon: 307.0,    // Approximate galactic longitude  
    name: "Great Attractor"
};

// Your MBT root
const mbt_root = {
    lat: -70.0,
    lon: 180.0,
    name: "MBT Curvature Root"
};

// Calculate angular separations
function angularSeparation(obj1, obj2) {
    const deg_to_rad = Math.PI / 180;
    const lat1 = obj1.lat * deg_to_rad;
    const lon1 = obj1.lon * deg_to_rad;
    const lat2 = obj2.lat * deg_to_rad;
    const lon2 = obj2.lon * deg_to_rad;
    
    return Math.acos(
        Math.sin(lat1) * Math.sin(lat2) + 
        Math.cos(lat1) * Math.cos(lat2) * Math.cos(lon1 - lon2)
    ) / deg_to_rad;
}

const cmb_ga_separation = angularSeparation(cmb_cold_spot, great_attractor);
const mbt_ga_separation = angularSeparation(mbt_root, great_attractor);
const mbt_cmb_separation = angularSeparation(mbt_root, cmb_cold_spot);

console.log("\n📍 COORDINATE ANALYSIS:");
console.log("======================");
console.log(`${cmb_cold_spot.name}: (${cmb_cold_spot.lat}°, ${cmb_cold_spot.lon}°)`);
console.log(`${great_attractor.name}: (${great_attractor.lat}°, ${great_attractor.lon}°)`);
console.log(`${mbt_root.name}: (${mbt_root.lat}°, ${mbt_root.lon}°)`);

console.log("\n🎯 ANGULAR SEPARATIONS:");
console.log("=======================");
console.log(`CMB Cold Spot ↔ Great Attractor: ${cmb_ga_separation.toFixed(1)}°`);
console.log(`MBT Root ↔ Great Attractor: ${mbt_ga_separation.toFixed(1)}°`);
console.log(`MBT Root ↔ CMB Cold Spot: ${mbt_cmb_separation.toFixed(1)}°`);

// Check for alignment patterns
const alignment_threshold = 30; // degrees

console.log("\n🔍 ALIGNMENT ANALYSIS:");
console.log("======================");
if (cmb_ga_separation < alignment_threshold) {
    console.log(`🚨 POTENTIAL ALIGNMENT: CMB Cold Spot and Great Attractor are only ${cmb_ga_separation.toFixed(1)}° apart!`);
} else {
    console.log(`CMB Cold Spot and Great Attractor separation: ${cmb_ga_separation.toFixed(1)}° (not closely aligned)`);
}

if (mbt_ga_separation < alignment_threshold) {
    console.log(`🚨 POTENTIAL ALIGNMENT: MBT Root and Great Attractor are only ${mbt_ga_separation.toFixed(1)}° apart!`);
} else {
    console.log(`MBT Root and Great Attractor separation: ${mbt_ga_separation.toFixed(1)}° (not closely aligned)`);
}

Result

